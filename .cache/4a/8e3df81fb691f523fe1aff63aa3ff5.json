@media screen and (min-width: 768px) and (max-width: 1199px) {\r\n  .container {\r\n    width: 768px;\r\n    padding-left: 34px;\r\n    padding-right: 34px;\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 1200px) {\r\n  .container {\r\n    width: 1090px;\r\n  }\r\n}\r\n","sass\\components\\_button.scss":".button {\r\n  @include font(8px, 400, 1.25, 0.04em);\r\n  font-weight: normal;\r\n  text-align: center;\r\n  color: #ffffff;\r\n  background-color: #d41443;\r\n  border-radius: 20px;\r\n  width: 62px;\r\n  height: 24px;\r\n  border: none;\r\n\r\n  @media screen and(min-width: 1200px) {\r\n    & {\r\n      @include font(14px, 400, 1.29, 0.04em);\r\n      width: 104px;\r\n      height: 40px;\r\n    }\r\n  }\r\n}\r\n\r\n.button-white {\r\n  width: 82px;\r\n  color: #d41443;\r\n  background-color: #ffffff;\r\n\r\n  @media screen and(min-width: 1200px) {\r\n    & {\r\n      width: 138px;\r\n    }\r\n  }\r\n}\r\n","sass\\utils\\_mixin.scss":"@mixin font($fs, $fw, $lh, $ls) {\r\n  font-size: $fs;\r\n  font-weight: $fw;\r\n  line-height: $lh;\r\n  letter-spacing: $ls;\r\n}\r\n","sass\\layout\\_hero.scss":".hero {\r\n  background-color: var(--primary-bg-color);\r\n\r\n  &__title {\r\n    font-family: 'Titan One' sans-serif;\r\n    @include font(22px, 400, 1.14, 0.04em);\r\n    text-transform: uppercase;\r\n    color: #ffffff;\r\n\r\n    @media screen and(min-width: 768px) {\r\n      & {\r\n        @include font(26px, 400, 1.15, 0.04em);\r\n      }\r\n    }\r\n    @media screen and(min-width: 1200px) {\r\n      & {\r\n        @include font(38px, 400, 1.16, 0.04em);\r\n      }\r\n    }\r\n  }\r\n\r\n  &__accent-title {\r\n    font-family: Titan One;\r\n    @include font(48px, 400, 0.965, 0.04em);\r\n    text-transform: uppercase;\r\n    color: #d41443;\r\n\r\n    @media screen and(min-width: 767px) {\r\n      & {\r\n        @include font(34px, 400, 0.905, 0.04em);\r\n      }\r\n    }\r\n\r\n    @media screen and(min-width: 767px) {\r\n      & {\r\n        @include font(58px, 400, 0.905, 0.04em);\r\n      }\r\n    }\r\n  }\r\n\r\n  &__btn-list {\r\n    display: flex;\r\n  }\r\n  &__btn-item:not(:last-child) {\r\n    margin-right: 9px;\r\n    @media screen and(min-width: 768px) {\r\n      & {\r\n        margin-right: 8px;\r\n      }\r\n    }\r\n    @media screen and(min-width: 1200px) {\r\n      & {\r\n        margin-right: 15px;\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"dbb3bf3cc58fe7073572b38d4e148c74","cacheData":{"env":{}}}